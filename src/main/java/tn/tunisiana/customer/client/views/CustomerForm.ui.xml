<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.datepicker.client"
	xmlns:cus="urn:import:tn.tunisiana.customer.client.views.customui">
	<ui:style src="styles/CustomerManager.css">
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel styleName="{style.htmlPannel}">
		<g:Grid>
			<g:row>
				<g:customCell>
					<g:Label horizontalAlignment="ALIGN_LEFT" text="Nom" width="33px" />
				</g:customCell>
				<g:customCell>

					<g:TextBox textAlignment="ALIGN_RIGHT" ui:field="nom" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Prénom" width="120px" />
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="prenom" />


				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Nationalité" width="120px" />
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="nationalite" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Civilité" width="120px" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:RadioButton name="civilite" ui:field="mr" text="M" />
				</g:customCell>
				<g:customCell>
					<g:RadioButton name="civilite" ui:field="mme" text="Mme" />
				</g:customCell>
				<g:customCell>
					<g:RadioButton name="civilite" ui:field="mlle" text="Mlle" />
				</g:customCell>
				
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Téléphone fixe" width="120px" />
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="tel" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Adresse email" width="120px" />
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="email" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Piéce d'identité" width="120px" />
				</g:customCell>
				<g:customCell>
					<g:RadioButton name="ident" ui:field="cin"
						text="Carte d&apos;identité Nationale" />
				</g:customCell>
				<g:customCell>
					<g:RadioButton name="ident" ui:field="passeport"
						text="Passeport" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Numéro Pièce d'identité" width="120px" />
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="numId" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Date de naissance" width="120px" />
				</g:customCell>
				<g:customCell>
					<!-- <p1:DatePicker ui:field="birthday" width="142px" height="43px"/> -->
					<cus:DatePickerWithYearSelector
						ui:field="birthday" width="142px" height="43px" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Adresse: " width="121px" />
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="adresse" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Ville" width="121px" />
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="ville" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="code postal" width="122px" />
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="postcode" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Gouvernorat" width="122px" />
				</g:customCell>
				<g:customCell>
					<g:ListBox visibleItemCount="1" height="27px" ui:field="gouvernorat" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Niveau d'études" width="120px" />
				</g:customCell>
				<g:customCell>
					<g:RadioButton name="nivEt" ui:field="primaire" text="Primaire" />
				</g:customCell>
				<g:customCell>
					<g:RadioButton name="nivEt" ui:field="sec" text="Secondaire" />
				</g:customCell>
				<g:customCell>
					<g:RadioButton name="nivEt" ui:field="sup" text="Superieur" />

				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Proféssion" width="120px" />
					</g:customCell>
					<g:customCell>
					<g:RadioButton name="profession" ui:field="sal" text="Salarié" />
					</g:customCell>
					<g:customCell>
					<g:RadioButton name="profession" ui:field="liberal"
						text="Proféssion Libérale" />
						</g:customCell>
					<g:customCell>
					<g:RadioButton name="profession" ui:field="etudiant"
						text="Etudiant" />
						</g:customCell>
					<g:customCell>
					<g:Label text="Autre" width="120px" />
					</g:customCell>
					<g:customCell>

					<g:TextBox ui:field="autreProfession" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Raison d'achat" width="120px" />
				</g:customCell>
				<g:customCell>
					<g:RadioButton formValue="1" name="raisAchat"
						ui:field="perso" text="Usage Personnel" />
				</g:customCell>
				<g:customCell>
					<g:RadioButton formValue="2" name="raisAchat"
						ui:field="pro" text="Usage Professionnel" />
				</g:customCell>
				<g:customCell>
					<g:RadioButton formValue="3" name="raisAchat"
						ui:field="autrui" text="Pour Autrui" />

				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label
						text="Si vous n'êtes pas l'utilisateur veuillez remplir les champs suivants"
						width="120px" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Niveau d'études" width="120px" />
				</g:customCell>
				<g:customCell>
					<g:RadioButton name="nivEtudUtil" ui:field="salUtil"
						text="Salarié" />
				</g:customCell>
				<g:customCell>
					<g:RadioButton name="nivEtudUtil" ui:field="liberalUtil"
						text="Proféssion Libérale" />
				</g:customCell>
				<g:customCell>
					<g:RadioButton name="nivEtudUtil" ui:field="etudiantUtil"
						text="Etudiant" />
				</g:customCell>
				<g:customCell>
					<g:Label text="Autre" width="120px" />
				</g:customCell>
				<g:customCell>

					<g:TextBox ui:field="autreNivEtudes" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>

					<g:Label text="Nom" width="120px" />
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="nomUtilisateur" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Prénom" width="120px" />
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="prenomUtilisateur" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Date de naissance" width="120px" />
				</g:customCell>
				<g:customCell>
					<!-- <p1:DatePicker ui:field="birthday" width="142px" height="43px"/> -->
					<cus:DatePickerWithYearSelector
						ui:field="birthdayUtilisateur" width="142px" height="43px" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Nationalité" width="120px" />
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="nationaliteUtilisateur" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Civilité" width="120px" />
				</g:customCell>
				<g:customCell>

					<g:RadioButton name="civiliteUtil" ui:field="mUtil"
						text="M" />
				</g:customCell>
				<g:customCell>
					<g:RadioButton name="civiliteUtil" ui:field="mmeUtil"
						text="Mme" />
				</g:customCell>
				<g:customCell>
					<g:RadioButton name="civiliteUtil" ui:field="mlleUtil"
						text="Mlle" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>

					<g:Label text="Adresse: " width="121px" />
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="adresseUtilisateur" />

				</g:customCell>
			</g:row>
			<g:row>


				<g:customCell>
					<g:Label text="Ville" width="121px" />
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="villeUtilisateur" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="code postal" width="122px" />
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="postCodeUtilisateur" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Gouvernorat" width="122px" />
				</g:customCell>
				<g:customCell>
					<g:ListBox visibleItemCount="1" height="27px"
						ui:field="gouvernoratUtilisateur" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Achat accompagné? si oui remplir les champs suivants"
						width="120px" />
				</g:customCell>
				<g:customCell>

					<g:RadioButton name="achatAccomp" ui:field="oui"
						text="Oui" />
				</g:customCell>
				<g:customCell>
					<g:RadioButton name="achatAccomp" ui:field="non"
						text="Non" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:RadioButton name="accompag" ui:field="Ami" text="Ami(es)" />
				</g:customCell>
				<g:customCell>
					<g:RadioButton name="accompag" ui:field="parents"
						text="Parents" />
				</g:customCell>
				<g:customCell>
					<g:RadioButton name="accompag" ui:field="vendeur"
						text="Vendeur" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Raison du choix de l'opérateur" width="120px" />
				</g:customCell>
				<g:customCell>
					<g:RadioButton name="raisonChoix" ui:field="image"
						text="Image" />
				</g:customCell>
				<g:customCell>
					<g:RadioButton name="raisonChoix" ui:field="prix"
						text="Prix" />
				</g:customCell>
				<g:customCell>
					<g:RadioButton name="raisonChoix" ui:field="qos"
						text="Qualité de service" />
				</g:customCell>
				<g:customCell>
					<g:RadioButton name="raisonChoix" ui:field="offre"
						text="Offre" />
				</g:customCell>
				<g:customCell>
					<g:Label text="Autre" width="120px" />
				</g:customCell>
				<g:customCell>

					<g:TextBox ui:field="AutreRaison" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>

					<g:Label text="Ancien Opérateur" width="122px" />
				</g:customCell>
				<g:customCell>
					<g:ListBox visibleItemCount="5" height="27px" ui:field="operateur" />
				</g:customCell>
			</g:row>
		</g:Grid>
		<g:Button styleName="{style.important}" ui:field="button"
			text="Valider" />
	</g:HTMLPanel>
</ui:UiBinder> 



