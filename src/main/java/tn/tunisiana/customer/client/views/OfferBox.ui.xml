<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>

	<g:HTMLPanel width="500" height="300">
	
		<g:StackPanel ui:field="offersStack"></g:StackPanel>
		<g:Label ui:field="offerLabel"></g:Label>
		<g:Label visible="false" ui:field="errorMessage" text="Pas d'offres pour ce client"></g:Label>
		<g:Button ui:field="close" text="fermer"></g:Button>


	</g:HTMLPanel>
</ui:UiBinder> 